{
    "name":"Youtuber++",
    "version":"0.0.1",
    "manifest_version":2,
    "description":"Yazılım ve Girişim Youtube Kanalı İçin Özel Özellikleri Aktif Eden Chrome Eklentisi",
    "homepage_url":"https://hayatikodla.net",
    "icons":{
        "16":"icons/favicon-16x16.png",
        "48":"icons/android-icon-48x48.png",
        "144":"icons/android-icon-144x144.png"
    },
    "default_locale":"en",
    "background":{
        "scripts":[
            "js/background.js"
        ],
        "persistent":true
    },
    "permissions":[
        "chrome://favicon/",
        "contentSettings",
        "contextMenus",
        "notifications",
        "https://*.youtube.com/watch?v=*",
        "https://youtube.com/watch?v=*"
    ],
    "web_accessible_resources": [
        "css/youtuber++.css"
    ],
    "content_scripts":[
        {
            "matches":[
                "https://*.youtube.com/*"
            ],
            "css":[
                "css/youtuber++.css"
            ],
            "js":[
                "js/jquery/jquery.js",
                "js/youtuber++.js"
            ]
        }
    ]
}